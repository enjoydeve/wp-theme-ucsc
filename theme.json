{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"name": "header",
			"area": "header"
		},
		{
			"name": "footer",
			"area": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/paragraph": {
				"color": {
					"custom": false,
					"background": false,
					"text": true,
					"link": false
				},
				"typography": {
					"customFontSize": true
				}
			},
			"core/navigation": {
				"typography": {
					"customFontSize": true
				}
			},
			"core/heading": {
				"typography": {
					"customFontSize": false
				},
				"color": {
					"custom": false
				}
			}
		},
		"color": {
			"custom": false,
			"link": true,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"slug": "white",
					"color": "#fff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000",
					"name": "Black"
				},
				{
					"slug": "ucsc-primary-blue",
					"color": "#003c6c",
					"name": "Primary blue"
				},
				{
					"slug": "ucsc-primary-yellow",
					"color": "#fdc700",
					"name": "Primary yellow"
				},
				{
					"slug": "ucsc-secondary-blue",
					"color": "#006aad",
					"name": "Secondary blue"
				},
				{
					"slug": "ucsc-light-blue",
					"color": "#13a5dc",
					"name": "Light blue"
				},
				{
					"slug": "ucsc-blue-green",
					"color": "#13a5dc",
					"name": "Blue-green"
				},
				{
					"slug": "ucsc-orange",
					"color": "#f29813",
					"name": "UCSC orange"
				},
				{
					"slug": "ucsc-light-orange",
					"color": "#ffbf00",
					"name": "UCSC light orange"
				},
				{
					"slug": "ucsc-lime-green",
					"color": "#93c02d",
					"name": "UCSC lime green"
				},
				{
					"slug": "ucsc-rubine-red",
					"color": "#da216d",
					"name": "UCSC rubine red"
				}
			]
		},
		"layout": {
			"contentSize": "67.5rem",
			"wideSize": "90rem"
		},
		"spacing": {
			"blockGap": true,
			"padding": true,
			"margin": true,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"lineHeight": true,
			"customFontSize": false,
			"fontSizes": [
				{
					"slug": "tiny",
					"size": "0.875em",
					"name": "Tiny"
				},
				{
					"slug": "small",
					"size": "1em",
					"name": "Small"
				},
				{
					"slug": "normal",
					"size": "16px",
					"name": "Normal"
				},
				{
					"slug": "medium",
					"size": "1.125em",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.25em",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "1.5em",
					"name": "Extra Large"
				},
				{
					"slug": "huge",
					"size": "2em",
					"name": "Huge"
				},
				{
					"slug": "gigantic",
					"size": "6em",
					"name": "Gigantic"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"slug": "system-font",
					"name": "System Font"
				},
				{
					"fontFamily": "Roboto, sans-serif",
					"slug": "roboto",
					"name": "Roboto"
				},
				{
					"fontFamily": "Roboto Condensed, sans-serif",
					"slug": "roboto-condensed",
					"name": "Roboto Condensed"
				}
			]
		},
		"custom": {
			"line-height": {
				"tiny": 1.15,
				"small": 1.2,
				"body": 1.428571429
			},
			"spacing": {
				"baseline": "2rem"
			},
			"color": {
				"link": "#01589d",
				"link-decoration": "#ffc500",
				"heading-text": "#003662",
				"default-text": "#222222",
				"flourish": "#a2a09c"
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "2em"
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--custom--color--default-text)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				}
			},
			"h1": {
				"color": {
					"text": "var(--wp--custom--color--heading-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "600"
				},
				"spacing": {
					"margin": {
						"top": "1em",
						"right": "0",
						"bottom": "0.5em",
						"left": "0"
					}
				}
			},
			"h2": {
				"color": {
					"text": "var(--wp--custom--color--heading-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600"
				},
				"spacing": {
					"margin": {
						"top": "1em",
						"right": "0",
						"bottom": "0.5em",
						"left": "0"
					}
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--custom--color--heading-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600"
				},
				"spacing": {
					"margin": {
						"top": "1.15em",
						"right": "0",
						"bottom": "0.5em",
						"left": "0"
					}
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--custom--color--heading-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600"
				},
				"spacing": {
					"margin": {
						"top": "1.25em",
						"right": "0",
						"bottom": "0.5em",
						"left": "0"
					}
				}
			},
			"h5": {
				"color": {
					"text": "var(--wp--custom--color--heading-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "600",
					"lineHeight": "1"
				},
				"spacing": {
					"margin": {
						"top": "1.5em",
						"right": "0",
						"bottom": "0.25em",
						"left": "0"
					}
				}
			},
			"h6": {
				"color": {
					"text": "var(--wp--custom--color--heading-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--tiny)",
					"fontWeight": "600",
					"lineHeight": "1.1429"
				},
				"spacing": {
					"margin": {
						"top": "1.7em",
						"right": "0",
						"bottom": "0.15em",
						"left": "0"
					}
				}
			}
		},
		"blocks": {
			"core/paragraph": {
				"spacing": {
					"margin": {
						"top": "0.75em",
						"right": "0",
						"bottom": "0.75em",
						"left": "0"
					}
				},
				"typography": {
					"lineHeight": "1.5",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "'Roboto Condensed', sans-serif",
					"fontWeight": "600",
					"lineHeight": "1.2",
					"textTransform": "uppercase"
				}
			}
		}
	},
	"customTemplates": []
}
