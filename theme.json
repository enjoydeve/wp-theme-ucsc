{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "templateParts": [
    {
      "name": "header",
      "area": "header"
    },
    {
      "name": "footer",
      "area": "footer"
    }
  ],
  "settings": {
    "blocks": {
      "core/paragraph": {
        "color": {
          "custom": false,
          "palette": [
            {
              "name": "Black",
              "slug": "black",
              "color": "#000000"
            }
          ]
        },
        "typography": {
          "customFontSize": false
        }
      },
      "core/navigation": {
        "typography": {
          "customFontSize": true
        }
      },
      "core/heading": {
        "typography": {
          "customFontSize": false
        },
        "color": {
          "custom": false
        }
      }
    },
    "color": {
      "custom": false,
      "link": true,
      "defaultPalette": false,
      "palette": [
        {
          "slug": "white",
          "color": "#fff",
          "name": "White"
        },
        {
          "slug": "black",
          "color": "#000",
          "name": "Black"
        },
        {
          "slug": "heading-text",
          "color": "#003662",
          "name": "Heading Text"
        },
        {
          "slug": "default-text",
          "color": "#222",
          "name": "Default Text"
        },
        {
          "slug": "dark-grey",
          "color": "#23282D",
          "name": "Dark grey"
        },
        {
          "slug": "ucsc-primary-blue",
          "color": "#003c6c",
          "name": "Primary blue"
        },
        {
          "slug": "ucsc-primary-yellow",
          "color": "#fdc700",
          "name": "Primary yellow"
        },
        {
          "slug": "ucsc-secondary-blue",
          "color": "#006aad",
          "name": "Secondary blue"
        },
        {
          "slug": "ucsc-light-blue",
          "color": "#13a5dc",
          "name": "Light blue"
        },
        {
          "slug": "ucsc-link-blue",
          "color": "#01589d",
          "name": "Link blue"
        },
        {
          "slug": "ucsc-link-decoration",
          "color": "#ffc500",
          "name": "Link decoration"
        },
        {
          "slug": "ucsc-blue-green",
          "color": "#13a5dc",
          "name": "Blue-green"
        },
        {
          "slug": "ucsc-orange",
          "color": "#f29813",
          "name": "Slug orange"
        },
        {
          "slug": "ucsc-light-orange",
          "color": "#ffbf00",
          "name": "Slug light orange"
        },
        {
          "slug": "ucsc-lime-green",
          "color": "#93c02d",
          "name": "Slug lime green"
        },
        {
          "slug": "ucsc-rubine-red",
          "color": "#da216d",
          "name": "Slug rubine red"
        }
      ]
    },
    "layout": {
      "contentSize": "67.5rem",
      "wideSize": "100rem"
    },
    "spacing": {
      "blockGap": null,
      "customPadding": true,
      "customMargin": true,
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "customLineHeight": true,
      "customFontSize": false,
      "fontSizes": [
        {
          "slug": "tiny",
          "size": "0.875rem",
          "name": "Tiny"
        },
        {
          "slug": "small",
          "size": "1rem",
          "name": "Small"
        },
        {
          "slug": "medium",
          "size": "1.125rem",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "1.25rem",
          "name": "Large"
        },
        {
          "slug": "x-large",
          "size": "1.5rem",
          "name": "Extra Large"
        },
        {
          "slug": "huge",
          "size": "2rem",
          "name": "Huge"
        },
        {
          "slug": "gigantic",
          "size": "6rem",
          "name": "Gigantic"
        }
      ],
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "slug": "system-font",
          "name": "System Font"
        },
        {
          "fontFamily": "Roboto, sans-serif",
          "slug": "roboto",
          "name": "Roboto"
        }
      ]
    },
    "custom": {
      "line-height": {
        "tiny": 1.15,
        "small": 1.2,
        "body": 1.428571429
      },
      "spacing": {
        "unit": "20px",
        "horizontal": "25px",
        "vertical": "30px"
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--white)",
      "text": "var(--wp--preset--color--default-text)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--roboto)",
      "fontSize": "var(--wp--preset--font-size--small)",
      "textDecoration": "none",
      "lineHeight": "var(--wp--custom--line-height--body)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--ucsc-link-blue)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontWeight": "600"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "600"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--tiny)",
          "fontWeight": "600"
        }
      }
    },
    "blocks": {
      "core/heading": {
        "color": {
          "text": "var(--wp--preset--color--heading-text)"
        },
        "spacing": {
          "margin": {
            "top": "1em",
            "right": "0",
            "bottom": "0.65em",
            "left": "0"
          }
        }
      },
      "core/paragraph": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "spacing": {
          "margin": {
            "top": "1em",
            "right": "0",
            "bottom": "1em",
            "left": "0"
          }
        }
      }
    }
  },
  "customTemplates": [

  ]
}
